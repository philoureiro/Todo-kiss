<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="496.0" prefWidth="677.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EditTaskController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Editar tarefa">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Text>
      <Label text="Descrição *">
         <padding>
            <Insets left="10.0" />
         </padding>
         <VBox.margin>
            <Insets left="20.0" top="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="txtDescription" prefHeight="0.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="5.0" left="20.0" right="20.0" />
         </VBox.margin></TextField>
      <Label layoutX="10.0" layoutY="69.0" text="Descrição2">
         <padding>
            <Insets left="10.0" />
         </padding>
         <VBox.margin>
            <Insets left="20.0" top="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="txtSubDescription" prefHeight="67.0" prefWidth="677.0">
         <padding>
            <Insets top="20.0" />
         </padding>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Label layoutX="10.0" layoutY="111.0" text="Status *">
         <padding>
            <Insets left="10.0" />
         </padding>
         <VBox.margin>
            <Insets left="20.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ChoiceBox fx:id="choiceStatus" prefHeight="25.0" prefWidth="259.0">
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </ChoiceBox>
      <Label layoutX="10.0" layoutY="165.0" text="Data *">
         <padding>
            <Insets left="10.0" />
         </padding>
         <VBox.margin>
            <Insets left="20.0" top="5.0" />
         </VBox.margin>
      </Label>
      <DatePicker fx:id="maxDate">
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </DatePicker>
      <Label layoutX="10.0" layoutY="207.0" text="Categoria *">
         <padding>
            <Insets left="10.0" />
         </padding>
         <VBox.margin>
            <Insets left="20.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ChoiceBox fx:id="choiceCategory" prefHeight="25.0" prefWidth="259.0">
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </ChoiceBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#onConfirmButtonClicked" prefHeight="25.0" prefWidth="152.0" text="Confirmar">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelButtonClicked" prefHeight="25.0" prefWidth="173.0" text="Cancelar">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
